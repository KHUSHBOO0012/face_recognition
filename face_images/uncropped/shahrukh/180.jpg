		function facebookprocess_autologin(facebookprocess_id) {

			if(facebookprocess_id==0)

			{

				allow_login = getCookie('facebook_autologin');

				document.getElementById('id_remember_me').style.display = 'none';

				if(allow_login!=2)

					mooFaceboxExShow("", "#facebookprocess_autologin_prompt", 570)

			}

			else

			{

				var ajax = new SEMods.Ajax(facebookprocess_autologin_onSuccess);

				var params = 'task=autologin&facebookprocess_id='+facebookprocess_id;

				ajax.post(facebookprocess_endpoint, params)

			}

		}

		function facebookprocess_autologin_onSuccess (obj, responseText) {

			var r = [];

			try {

				r = eval('(' + responseText + ')')

			} catch(e) {

				r.status = 1

			};

			if (r.status == 0) {

				if(r.autologin == 0) {

					// show dialog

					mooFaceboxExShow("", "#facebookprocess_autologin_prompt", 570)

				} else if(r.autologin == 1) {

					// autologin

					parent.facebookprocess.login_facebook(document.location, force_next);

				}

				// otherwise autologin is off by user

			} else {

			}

		}

		function facebookprocess_autologin_confirmed() {

			var checkbox = _mooFaceboxEx.faceboxEl.getElement('.openidconnect_autologin_remember');

			var checked = checkbox.checked;

			mooFaceboxExClose();

			if(facebookprocess_id==0)

			{

				document.getElementById('id_remember_me').style.display = '';

				parent.facebookprocess.login_facebook(document.location, force_next);

			}

			else

			{

				var complete_login = function(obj, responseText) { parent.facebookprocess.login_facebook(document.location, force_next); };

				if(checked) {

					var ajax = new SEMods.Ajax(complete_login,complete_login);

					var params = 'task=autologinnexttime&facebookprocess_id='+facebookprocess_id;

					ajax.post(facebookprocess_endpoint, params);

				} else {

					parent.facebookprocess.login_facebook(document.location, force_next);



				}

			}

		}

		function facebookprocess_autologin_cancel() {

			var checkbox = _mooFaceboxEx.faceboxEl.getElement('.openidconnect_autologin_remember');

			var checked = checkbox.checked;

			mooFaceboxExClose();

			if(facebookprocess_id==0)

			{

				document.getElementById('id_remember_me').style.display = '';

				setCookie('facebook_autologin',2);

			}

			else

			{

			  var ajax = new SEMods.Ajax();

			  var params = 'task=autologinsuppress&facebookprocess_id='+facebookprocess_id+'&autologinremember=' + (checked ? 1 : 0);

			  ajax.post(facebookprocess_endpoint, params);

			}

		}

		function getCookie(c_name)

		{

			var i,x,y,ARRcookies=document.cookie.split(";");

			for (i=0;i<ARRcookies.length;i++)

			{

				x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));

				y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);

				x=x.replace(/^\s+|\s+$/g,"");

				if (x==c_name)

				{

					return unescape(y);

				}

			}

		}

		function setCookie(c_name,value,exdays)

		{

			var exdate=new Date();

			exdate.setDate(exdate.getDate() + exdays);

			var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());

			document.cookie=c_name + "=" + c_value;

		}

